#!/bin/bash 


PATH=$PATH:/usr/libexec/git-core:$(pwd)/tests/t/helper

export PERL5LIB=/usr/share/perl5/
export GIT_TEST_INSTALLED=$(pwd)/tests/t

export GIT_SKIP_TESTS="t2022.4 t3404.52 t4200.16 t4200.17 t4200.30 t5000.28 t1700.18 t1700.19"
export GIT_SKIP_TESTS="$GIT_SKIP_TESTS t5100.21 t5100.24 t5100.31 t5304 t5400.12 t5801"
export GIT_SKIP_TESTS="$GIT_SKIP_TESTS t6022.30 t6022.32 t6022.34 t6022.36 t6022.38 t6500.9 t6500.10"
export GIT_SKIP_TESTS="$GIT_SKIP_TESTS t7508.112 t7701.4 t9300.117"

cd tests/t

prove --timer --jobs 15 ./t[0-9]*.sh
